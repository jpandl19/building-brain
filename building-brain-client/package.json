{
  "name": "building-brain-client",
  "version": "0.0.1",
  "description": "Chat with your building and fight out what you need to fix and where!",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.js",
    "dev:sspa": "_SINGLE_SPA_=single-spa vite --config vite.config.js",
    "build": "vite --config vite.config.js build",
    "build:sspa": "_SINGLE_SPA_=single-spa vite --config vite.config.js build",
    "build:sspa:watch": "_SINGLE_SPA_=single-spa vite --config vite.config.js --watch build",
    "lint": "eslint ./src --ext js,ts,tsx",
    "lint:fix": "eslint ./src --fix --ext js,ts,tsx",
    "preview": "vite preview",
    "preview:sspa": "_SINGLE_SPA_=single-spa vite preview",
    "test": "exit 0"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.0.1",
    "@datadog/browser-rum": "^4.39.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "@mui/x-data-grid": "^6.10.1",
    "@mui/x-date-pickers": "^6.9.2",
    "@reduxjs/toolkit": "^1.8.5",
    "@stripe/react-stripe-js": "^2.1.0",
    "axios": "^1.3.4",
    "date-fns": "^2.30.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.4.1",
    "vite-plugin-static-copy": "^0.16.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
